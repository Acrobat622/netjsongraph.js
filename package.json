{
  "name": "netjsongraph.js",
  "version": "0.1.2",
  "description": "NetJson format data visualization",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server  --config webpack.config.js",
    "build": "./node_modules/.bin/webpack --config webpack.config.prod.js",
    "test": "./node_modules/.bin/jest",
    "test:cover": "npm test --coverage",
    "test:cover:travis": "npm run test:cover && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "prepublish": "npm run build"
  },
  "repository": "https://github.com/netjson/netjsongraph.js.git",
  "author": [
    "Federico Capoano <federico.capoano@gmail.com> (http://nemesisdesign.net/)",
    "GeekPlux <geekplux@gmail.com> (http://geekplux.com/)"
  ],
  "contributors": [],
  "license": "BSD-3-Clause",
  "keywords": [
    "netjson",
    "network",
    "data",
    "visualization"
  ],
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.7.1",
    "jest": "^20.0.4",
    "json-loader": "^0.5.4",
    "lodash.debounce": "^4.0.8",
    "postcss-loader": "^0.13.0",
    "precss": "^1.4.0",
    "stats.js": "^0.17.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "worker-loader": "^0.8.1"
  },
  "dependencies": {
    "d3": "^4.9.1",
    "normalize.css": "^7.0.0",
    "three": "^0.85.2"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "roots": [
      "<rootDir>/tests/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/tests/__mocks__/styleMock.js"
    }
  }
}
